  "tags": [],
  "servers": [],
  "components": {
    "schemas": {
      "UpdatePasswordDto": {
        "type": "object",
        "properties": {
          "oldPassword": {
            "type": "string",
            "example": "nickname12345!",
            "description": "The current password of the user."
          },
          "newPassword": {
            "type": "string",
            "example": "NewPassword456!",
            "description": "The new password for the user. Must not be empty."
          }
        },
        "required": [
          "oldPassword",
          "newPassword"
        ]
      },
      "CreateTrackDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Flowers"
          },
          "artistId": {
            "type": "string",
            "format": "uuid",
            "example": "123e4567-e89b-12d3-a456-426614174000",
            "nullable": true
          },
          "albumId": {
            "type": "string",
            "format": "uuid",
            "example": "123e4567-e89b-12d3-a456-426614174000",
            "nullable": true
          },
          "duration": {
            "type": "number",
            "example": 202
          }
        },
        "required": [
          "name",
          "artistId",
          "albumId",
          "duration"
        ]
      },
      "UpdateTrackDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Updated Song Name"
          },
          "artistId": {
            "type": "string",
            "format": "uuid",
            "example": "123e4567-e89b-12d3-a456-426614174000"
          },
          "albumId": {
            "type": "string",
            "format": "uuid",
            "example": "123e4567-e89b-12d3-a456-426614174000"
          },
          "duration": {
            "type": "number",
            "example": 320
          }
        }
      },
      "CreateAlbumDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Name of the album",
            "example": "The Dark Side of the Moon"
          },
          "year": {
            "type": "number",
            "description": "Release year of the album",
            "minimum": 1900,
            "maximum": 2024,
            "example": 1973
          },
          "artistId": {
            "type": "string",
            "format": "uuid",
            "description": "Artist ID associated with the album",
            "example": "123e4567-e89b-12d3-a456-426614174000"
          }
        },
        "required": [
          "name",
          "year"
        ]
      },
      "UpdateAlbumDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Updated Album Name"
          },
          "year": {
            "type": "number",
            "example": 2023,
            "minimum": 1900,
            "maximum": 2024
          },
          "artistId": {
            "type": "string",
            "format": "uuid",
            "example": "123e4567-e89b-12d3-a456-426614174000"
          }
        }
      }
    }
  }
